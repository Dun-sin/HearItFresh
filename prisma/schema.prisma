generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model User {
  userId          String  @id @map("user_id")
  displayName     String  @map("display_name")
  profileImageUrl String? @map("profile_image_url")
  history         Json?

  @@map("users")
}